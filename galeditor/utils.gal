lowkey utils.save_lines{}
    fax lines = [GLOBAL_STATE "lines"]
    fax filename = [GLOBAL_STATE "filename"]
    fax contents = fire std.join(lines, "\n")
    fire std.write(filename, contents)
end

lowkey utils.min{a, b}
    foreal a < b
        rizzult a
    end
    rizzult b
end

lowkey utils.abs{x}
    foreal x < 0
        rizzult -x
    end
    rizzult x
end